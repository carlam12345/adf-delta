{
	"name": "df_gold_factventas",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_detalles_venta",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_cliente",
						"type": "DatasetReference"
					},
					"name": "dimCliente"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_producto",
						"type": "DatasetReference"
					},
					"name": "dimProducto"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_tiempo",
						"type": "DatasetReference"
					},
					"name": "dimTiempo"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_hechos_dim_factventas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sls_ord_num as string,",
				"          sls_prd_key as string,",
				"          sls_cust_id as string,",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string,",
				"          sls_sales as string,",
				"          sls_quantity as string,",
				"          sls_price as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT\\n    sls_ord_num,\\n    sls_prd_key,\\n    sls_cust_id,\\n    sls_order_dt,\\n    sls_ship_dt,\\n    sls_sales,\\n    sls_quantity,\\n    sls_price\\nFROM SILVER.Detalle_ventas\\n',",
				"     format: 'query') ~> source1",
				"source(output(",
				"          cst_id as string,",
				"          id_cliente as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT\\n  cst_id,      -- clave de negocio (texto)\\n  id_cliente   -- surrogate key (int)\\nFROM GOLD.DimCliente\\n',",
				"     format: 'query') ~> dimCliente",
				"source(output(",
				"          prd_key as string,",
				"          id_producto as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT\\n  prd_key,       -- clave de negocio (texto)\\n  id_producto    -- surrogate key (int)\\nFROM GOLD.DimProducto',",
				"     format: 'query') ~> dimProducto",
				"source(output(",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT\\n  sls_order_dt,\\n  sls_ship_dt,\\n  id_tiempo\\nFROM GOLD.DimTiempo;',",
				"     format: 'query') ~> dimTiempo",
				"source1, dimCliente lookup(sls_cust_id == cst_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, dimProducto lookup(sls_prd_key == prd_key,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimTiempo lookup(source1@sls_order_dt == dimTiempo@sls_order_dt",
				"     && source1@sls_ship_dt == dimTiempo@sls_ship_dt,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3 select(mapColumn(",
				"          sls_ord_num,",
				"          sls_prd_key,",
				"          sls_cust_id,",
				"          sls_order_dt = source1@sls_order_dt,",
				"          sls_ship_dt = source1@sls_ship_dt,",
				"          sls_sales,",
				"          sls_quantity,",
				"          sls_price,",
				"          cst_id,",
				"          prd_key,",
				"          sls_order_dt = dimTiempo@sls_order_dt,",
				"          sls_ship_dt = dimTiempo@sls_ship_dt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_venta as integer,",
				"          id_cliente as integer,",
				"          id_producto as integer,",
				"          id_tiempo as integer,",
				"          MontoVenta as string,",
				"          Cantidad as string,",
				"          PrecioUnitario as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     preSQLs:['TRUNCATE TABLE GOLD.FactVentas;\\n'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id_cliente,",
				"          id_producto,",
				"          id_tiempo,",
				"          MontoVenta,",
				"          Cantidad,",
				"          PrecioUnitario",
				"     )) ~> sink1"
			]
		}
	}
}