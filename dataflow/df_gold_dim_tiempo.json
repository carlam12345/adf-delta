{
	"name": "df_gold_dim_tiempo",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_detalles_ventas",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_tiempo",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          fecha as date,",
				"          anio as integer,",
				"          mes as integer,",
				"          dia as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT DISTINCT \\n    CAST(TRY_CONVERT(DATE, d.sls_order_dt, 112) AS DATE) AS fecha,\\n    YEAR(TRY_CONVERT(DATE, d.sls_order_dt, 112)) AS anio,\\n    MONTH(TRY_CONVERT(DATE, d.sls_order_dt, 112)) AS mes,\\n    DAY(TRY_CONVERT(DATE, d.sls_order_dt, 112)) AS dia\\nFROM SILVER.Detalle_ventas AS d\\nWHERE TRY_CONVERT(DATE, d.sls_order_dt, 112) IS NOT NULL\\n',",
				"     format: 'query') ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_tiempo as integer,",
				"          sls_order_dt as string,",
				"          sls_ship_dt as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          sls_order_dt = fecha,",
				"          sls_ship_dt = fecha",
				"     )) ~> sink1"
			]
		}
	}
}